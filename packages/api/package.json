{
    "name": "api",
    "version": "1.0.0",
    "description": "Rewrite for Bravo68web API",
    "main": "index.ts",
    "repository": "git@github.com:BRAVO68WEB/api-rewrite.git",
    "author": "Jyotirmoy Bandyopadhyaya [Bravo68] <jbandyopadhayaya@gmail.com>",
    "license": "MIT",
    "type": "module",
    "private": true,
    "dependencies": {
        "@aws-sdk/client-s3": "^3.226.0",
        "@discordjs/core": "^0.6.0",
        "@discordjs/rest": "^1.7.1",
        "@types/express": "^4.17.14",
        "axios": "^1.2.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "ejs": "^3.1.9",
        "envfile": "^6.18.0",
        "express": "^4.18.2",
        "form-data": "^4.0.0",
        "graphql": "^16.6.0",
        "graphql-request": "^5.0.0",
        "helmet": "^6.0.1",
        "joi": "^17.7.0",
        "mmdb-lib": "^2.0.2",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "multer-s3": "^3.0.1",
        "napi-nanoid": "^0.0.4",
        "node-cache": "^5.1.2",
        "nodemailer": "^6.8.0",
        "openid-client": "^5.4.0",
        "osu-api-extended": "^2.5.12",
        "ping": "^0.4.4",
        "redis": "^4.5.1",
        "typescript": "^4.9.3"
    },
    "scripts": {
        "dev": "concurrently \"npm run dev:express\" \"npm run dev:hasura\"",
        "dev:hasura": "cd hasura && hasura --skip-update-check  --envfile ../.env console",
        "dev:express": "cross-env NODE_ENV=development nodemon -x node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm index.ts --signal SIGKILL",
        "build": "tsc",
        "start": "node --es-module-specifier-resolution=node --loader ts-node/esm ./build/index.js",
        "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
        "fetch:schema": "bash ./bin/fetch-gql-schema.sh"
    },
    "devDependencies": {
        "@swc/core": "^1.3.23",
        "@swc/wasm": "^1.3.23",
        "@types/cors": "^2.8.13",
        "@types/morgan": "^1.9.3",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "concurrently": "^7.6.0",
        "cross-env": "^7.0.3",
        "graphqurl": "^1.0.1",
        "hasura-cli": "^2.15.1",
        "prettier": "^2.8.2",
        "ts-node": "^10.9.1"
    }
}
